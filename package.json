{
  "name": "ytp",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "watch": "rollup -w --config rollup.config.js",
    "test": "jest",
    "type": "dtsmake -s lib/ytp.js --dist lib/@type/index",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "dtsmake": "0.0.10",
    "jest": "^24.8.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3"
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "createjs": "^1.0.1",
    "typescript": "^3.3.1"
  },
  "main": "lib/ytp.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/Yuki-Sakaguchi",
  "repository": {
    "type": "git",
    "url": "git@github.com:Yuki-Sakaguchi/YTP.git"
  },
  "keywords": [
    "youtube"
  ],
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  }
}
